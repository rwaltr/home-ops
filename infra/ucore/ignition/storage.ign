{
  "ignition": {
    "version": "3.4.0"
  },
  "storage": {
    "files": [
      {
        "path": "/etc/modules-load.d/zfs.conf",
        "contents": {
          "compression": "",
          "source": "data:,zfs%0A"
        },
        "mode": 420
      },
      {
        "path": "/usr/local/bin/zfs-import-tank.sh",
        "contents": {
          "compression": "gzip",
          "source": "data:;base64,"
        },
        "mode": 493
      }
    ]
  },
  "systemd": {
    "units": [
      {
        "contents": "[Unit]\nDescription=Import ZFS pool 'tank'\nAfter=zfs.target\nBefore=local-fs.target\n\n[Service]\nType=oneshot\nRemainAfterExit=yes\nExecStart=/usr/local/bin/zfs-import-tank.sh\n\n[Install]\nWantedBy=multi-user.target\n",
        "enabled": true,
        "name": "zfs-import-tank.service"
      },
      {
        "enabled": true,
        "name": "zfs-scrub-weekly@tank.timer"
      },
      {
        "enabled": true,
        "name": "nfs-server.service"
      }
    ]
  }
}
